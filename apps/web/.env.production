# Web App Production Environment Variables
# Copy these to Vercel Environment Variables for Web App

NEXT_PUBLIC_API_URL=https://api.tradygo.in/api/v1
NEXT_PUBLIC_APP_NAME=TradyGo
NEXT_PUBLIC_APP_TYPE=web
NODE_ENV=production

# Demo Configuration
NEXT_PUBLIC_DEMO_MODE=true
NEXT_PUBLIC_DEMO_USER_EMAIL=user@tradygo.in
NEXT_PUBLIC_DEMO_USER_PASSWORD=User@12345!

# App URLs
NEXT_PUBLIC_ADMIN_URL=https://admin.tradygo.in
NEXT_PUBLIC_SELLER_URL=https://seller.tradygo.in
NEXT_PUBLIC_WEB_URL=https://tradygo.in

# E-commerce Features
NEXT_PUBLIC_ENABLE_CART=true
NEXT_PUBLIC_ENABLE_WISHLIST=true
NEXT_PUBLIC_ENABLE_REVIEWS=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true