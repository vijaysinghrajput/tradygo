# Production Environment Variables for All Frontend Apps
# Add these to Vercel Environment Variables for each app

# API Configuration
NEXT_PUBLIC_API_URL=https://api.tradygo.in/api/v1

# App Configuration
NEXT_PUBLIC_APP_NAME=TradyGo
NEXT_PUBLIC_APP_VERSION=1.0.0

# Environment
NODE_ENV=production

# Demo Configuration
NEXT_PUBLIC_DEMO_MODE=true
NEXT_PUBLIC_DEMO_ADMIN_EMAIL=admin@tradygo.in
NEXT_PUBLIC_DEMO_ADMIN_PASSWORD=Admin@12345!
NEXT_PUBLIC_DEMO_SUPER_ADMIN_EMAIL=sa@tradygo.in
NEXT_PUBLIC_DEMO_SUPER_ADMIN_PASSWORD=Admin@12345!

# Features
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_SENTRY=false

# URLs
NEXT_PUBLIC_ADMIN_URL=https://admin.tradygo.in
NEXT_PUBLIC_SELLER_URL=https://seller.tradygo.in
NEXT_PUBLIC_WEB_URL=https://tradygo.in